{
  "name": "swarm-console",
  "version": "1.0.0",
  "private": true,
  "docker": {
    "group": "app-swarm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --config .eslintrc.json",
    "test": "NEXT_PUBLIC_REMOTE_API_HOST=http://localhost:8080/api jest",
    "test:failed": "NEXT_PUBLIC_REMOTE_API_HOST=http://localhost:8080/api jest --silent --only-failures",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "fix": "eslint --fix src",
    "format:write": "npx prettier --write ."
  },
  "dependencies": {
    "@cgi-learning-hub/icons": "1.2.0",
    "@cgi-learning-hub/theme": "1.2.0",
