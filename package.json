    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^7.12.1",
    "@reduxjs/toolkit": "^2.2.7",
    "dayjs": "^1.11.12",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.1.3",
